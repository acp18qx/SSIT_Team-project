.toptab {
  background-color: #836db2; 
  overflow: hidden;
}

/* Style the links inside the navigation bar */
.toptab a {
  float: left;
  color:  #ffffff ;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}

/* Change the color of links on hover */
.toptab a:hover {
  background-color: #ddd;
  color: black;
}

/* Add a color to the active/current link */
.toptab a.active {
  background-color: #4CAF50;
  color: white;
}

